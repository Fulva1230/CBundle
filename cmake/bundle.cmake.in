file(GET_RUNTIME_DEPENDENCIES 
RESOLVED_DEPENDENCIES_VAR resolved_deps
UNRESOLVED_DEPENDENCIES_VAR unresolved_deps
DIRECTORIES ${search_path}
POST_INCLUDE_REGEXES ${search_path}
POST_EXCLUDE_REGEXES ".*"
EXECUTABLES ${target_exe}
)

message(search_path: ${search_path})

file(INSTALL ${target_exe} DESTINATION ${bundle_dir})
file(INSTALL ${resolved_deps} DESTINATION ${bundle_dir})
file(INSTALL ${resources_dir} DESTINATION ${bundle_dir})

message(resolved: ${resolved_deps})
message(unresolved: ${unresolved_deps})