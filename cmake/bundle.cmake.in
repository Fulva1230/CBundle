file(GET_RUNTIME_DEPENDENCIES 
RESOLVED_DEPENDENCIES_VAR resolved_deps
UNRESOLVED_DEPENDENCIES_VAR unresolved_deps
DIRECTORIES @CBUNDLE_DLL_PATH@
POST_INCLUDE_REGEXES @CBUNDLE_DLL_PATH@
POST_EXCLUDE_REGEXES ".*"
EXECUTABLES @CBUNDLE_TARGET_EXE_PATH@
)

message("search_path: " @CBUNDLE_DLL_PATH@)

file(INSTALL @CBUNDLE_TARGET_EXE_PATH@ DESTINATION @CBUNDLE_BUNDLE_DIR@)
file(INSTALL ${resolved_deps} DESTINATION @CBUNDLE_BUNDLE_DIR@)
file(INSTALL @CBUNDLE_RESOURCES_PATH@ DESTINATION @CBUNDLE_BUNDLE_DIR@)

message(resolved: ${resolved_deps})
message(unresolved: ${unresolved_deps})